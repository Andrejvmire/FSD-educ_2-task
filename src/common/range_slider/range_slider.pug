mixin range_slider(options)
    - let defaultOptions = {className: "range-slider", type: "single", min: 0, max: 100, from: 0, to: 50, currency: "ла"};
    - let {className, id, value, currency, comment_id} = Object.assign(defaultOptions, options);

    div(
        id= id
        class= className
        value=""
        data-type= type
        data-range= true
        data-value= value
        data-currency= currency
        data-comment_id= comment_id
    )